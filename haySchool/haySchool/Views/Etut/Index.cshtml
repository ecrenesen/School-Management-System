                @Html.DisplayFor(modelItem => item.etut_ders_adi)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.etut_sinav_id)
            </td>
          
           <td><a href="/Etut/EtutSil/@item.etut_id" class="btn btn-danger">Sil</a></td>
            <td><a href="/Etut/EtutGuncelle?id=@item.etut_id" class="btn btn-success">Update</a></td>
        </tr>
}
    </tbody>
</table>
<a href="/Etut/EtutEkle" class="btn btn-primary">Etüt Ekle</a>

