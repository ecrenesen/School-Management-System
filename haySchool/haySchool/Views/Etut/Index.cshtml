@model IEnumerable<haySchool.Models.Etut>

@{
    ViewData["Title"] = "Index";
}


@*<p>
    <a asp-action="Create">Create New</a>
</p>*@
<table class="table">
    <thead>
        <tr>
          @*  <th>
                ID
            </th>*@
            <th>
                Öğrenci ID
            </th>
            <th>
                Ders ID
            </th>
            <th>
               Sınav ID
            </th>
           
             <th></th>
            <th></th>
        </tr>
    </thead>
    <tbody>
@foreach (var item in Model) {
        <tr>
        @*    <td>
                @Html.DisplayFor(modelItem => item.etut_id)
            </td>*@
            <td>
                @Html.DisplayFor(modelItem => item.etut_ogrenci_adi)
            </td> 
